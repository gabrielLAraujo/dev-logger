{
  "name": "dev-logger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "deploy:prod": "npm run prisma:generate && npm run prisma:migrate && npm run build && npm run start",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "debug:prod": "cross-env NODE_ENV=production NODE_OPTIONS='--inspect' DOTENV_CONFIG_PATH=.env.local next start",
    "debug:test": "cross-env NODE_ENV=test NODE_OPTIONS='--inspect' DOTENV_CONFIG_PATH=.env.local jest --runInBand",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next node_modules/.cache"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@headlessui/react": "^2.2.1",
    "@heroicons/react": "^2.2.0",
    "@octokit/rest": "^21.1.1",
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.487.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "postcss": "^8.0.0",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0"
  }
}
